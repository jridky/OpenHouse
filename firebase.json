{
	"database": {
		"rules": "database.rules.json"
	},
	"hosting": {
		"public": "/",
		"ignore": [
			".firebaserc",
			"firebase.json",
			"database-rules.json",
			".idea",
			".git"
		],
		"cleanUrls": true,
		"redirects": [
			{
				"source" : "/",
				"destination" : "https://openhouse.redhat.com/cz/",
				"type" : 301
			},
			{
				"source" : "/index",
				"destination" : "https://openhouse.redhat.com/cz/",
				"type" : 301
			},
			{
				"source" : "/index.html",
				"destination" : "https://openhouse.redhat.com/cz/",
				"type" : 301
			}
		]
	}
}
