{
	"database": {
		"rules": "database.rules.json"
	},
	"hosting": {
		"public": "production",
		"ignore": [
			".firebaserc",
			"firebase.json",
			"database-rules.json",
            "composer.json",
            "package.json",
            "package-lock.json",
            "node-modules",
            "build-local",
            "production-old",
            "tasks",
            "webpack.mix.js",
            "composer.lock",
            "vendor",
            "source",
			".idea",
			".git"
		],
		"cleanUrls": true,
		"redirects": [
			{
				"source" : "/",
				"destination" : "https://openhouse.redhat.com/cz/",
				"type" : 301
			},
			{
				"source" : "/index",
				"destination" : "https://openhouse.redhat.com/cz/",
				"type" : 301
			},
			{
				"source" : "/index.html",
				"destination" : "https://openhouse.redhat.com/cz/",
				"type" : 301
			}
		]
	}
}
